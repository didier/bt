{
	"name": "miit",
	"version": "1.0.0",
	"description": "Modern Dating",
	"main": "index.js",
	"scripts": {
		"start": "node index.js",
		"get-components": "node scripts/get-components.js",
		"dev": "yarn get-components; NODE_ENV=development run-p sass start sync",
		"sync": "browser-sync start --no-notify --proxy 'localhost:3000' --files 'src/data/, src/styles/**/*, src/views/**/**/**/**'",
		"sass": "sass --watch --poll src/:public/",
		"lint": "prettier **/*.js --loglevel 'silent' & eslint --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/didiercatz/bt.git"
	},
	"keywords": [
		"date",
		"dating",
		"js",
		"html",
		"css",
		"mongodb",
		"progressive",
		"enhancement"
	],
	"author": "Didier Catz",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/didiercatz/bt/issues"
	},
	"homepage": "https://github.com/didiercatz/bt#readme",
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"browser-sync": "^2.26.7",
		"eslint": "^7.0.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-prettier": "^3.1.3",
		"husky": "^4.2.5",
		"nodemon": "^2.0.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.0.5",
		"prettier-eslint": "^9.0.1",
		"pretty-quick": "^2.0.1",
		"readdirp": "^3.4.0",
		"sass": "^1.26.5",
		"stylelint": "^13.5.0",
		"stylelint-8-point-grid": "^0.2.6",
		"stylelint-config-recommended": "^3.0.0",
		"stylelint-config-standard": "^20.0.0"
	},
	"dependencies": {
		"express": "^4.17.1",
		"handlebars": "^4.7.6",
		"hbs": "^4.1.1",
		"node-sass": "^4.14.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
